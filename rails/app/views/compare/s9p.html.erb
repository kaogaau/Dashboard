<div class="col-md-12">
                    <!--more statistics box start-->
                    <section class="panel">
                        <header class="panel-heading custom-tab dark-tab">
                            <ul class="nav nav-tabs">
                                <li class="active">
                                    <a href="" onclick="changeif(1)" data-toggle="tab">互動</a>
                                </li>
                                <li class="">
                                    <a href="" onclick="changeif(2)" data-toggle="tab">按讚</a>
                                </li>
                                <li class="">
                                    <a href="" onclick="changeif(3)" data-toggle="tab">留言</a>
                                </li>
                                <li class="">
                                    <a href="" onclick="changeif(4)" data-toggle="tab">分享</a>
                                </li>
                            </ul>
                            <script>
                            function changeif(a){
                            	if(a ==1){
                            	$("#theif").attr("src","/outer/s9p?cid=<%=params[:cid]%>&cate=engagement");
                            	}
                            	if(a==2){
                            		$("#theif").attr("src","/outer/s9p?cid=<%=params[:cid]%>&cate=like");
                            	}
                            	if(a==3){
                            		$("#theif").attr("src","/outer/s9p?cid=<%=params[:cid]%>&cate=comment");
                            	}
                            	if(a==4){
                            		$("#theif").attr("src","/outer/s9p?cid=<%=params[:cid]%>&cate=share");
                            	}
                            }
                            </script>
                        </header>
                        <div class="panel-body">
                            <div class="tab-content">
                                <div class="tab-pane active" id="home2">
                                      <div class="panel-body">
                            <div class="row">
                                
                                      <iframe id='theif' width="100%" height="570px" src="/outer/s9p?cid=<%=params[:cid]%>&cate=engagement" style="border: none"></iframe>

                            </div>
                        </div>
                                </div>
                                <div class="tab-pane" id="about2">
                                  <div class="panel-body">
                            <div class="row">
                                
                                      <iframe width="100%" height="300px" src="/outer/s9p?cid=<%=params[:cid]%>&cate=like" style="border: none"></iframe>

                            </div>
                        </div>
                                </div>
                                <div class="tab-pane" id="profile2">
                                    <div class="panel-body">
                            <div class="row">
                                
                                      <iframe width="100%" height="300px" src="/outer/s9p?cid=<%=params[:cid]%>&cate=comment" style="border: none"></iframe>

                            </div>
                        </div>
                                </div>
                                <div class="tab-pane" id="contact2">
                                	<div class="panel-body">
                            <div class="row">
                                
                                      <iframe width="100%" height="300px" src="/outer/s9p?cid=<%=params[:cid]%>&cate=share" style="border: none"></iframe>

                            </div>
                        </div>
                                </div>
                            </div>
                        </div>
                    </section>
                   
