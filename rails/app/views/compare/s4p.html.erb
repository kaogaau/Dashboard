                <%if params[:cid] != ''%>
 <div class="row">
 	
 	<div class="col-sm-12">
 		
                     <section class="panel">
                <table class="table table-bordered">
                            <thead>
                            	
                            <tr>
                                <th>FB</th>
                                <%@company.each do |c|%>
                                <th colspan="2"><%=@s4[c.to_i]["page_name"]%>
                                	<%if @s4[c.to_i]["top5"].include?("nil")%>
                                	<%else%>
                                	<a target="_blank" href="/outer/s4p_view?cid=<%=c.to_i%>&top1=<%= @s4[c.to_i]["top5"].keys[0]%>&top2=<%= @s4[c.to_i]["top5"].keys[1]%>&top3=<%= @s4[c.to_i]["top5"].keys[2]%>&top4=<%= @s4[c.to_i]["top5"].keys[3]%>&top5=<%= @s4[c.to_i]["top5"].keys[4] %>">(詳細內容)</a>
                                	<%end%>
                                	</th>
                                <%end%>
                            </tr>
                            
                            </thead>
                            <tbody>
                            	
                            	<% (0..4).each do |n| %>
                            <tr>
                                <td ><%= n + 1 %></td>
                                 <%@company.each do |c|%>
                                <td <%if @ppp.include?(@s4[c.to_i]["top5"].keys[n])%><%else%>style='color: red'<%end%>><%= @s4[c.to_i]["top5"].keys[n] %></td>
                                <td style="color:#000;font-weight: bold;text-shadow: 2px 2px 2px #fff;background: <%=getwarmcolor5(@s4[c.to_i]["top5"].values[n].to_i)%>">
                                	<%= @s4[c.to_i]["top5"].values[n] %></td>
                                <%end%>
                            </tr>
                            
                            <% end %>
                            
                            
                            </tbody>
                        </table>
                        
                        </section>
                        <h3>資料來源：粉絲團 最後更新時間：<%= @lasttime[0]["time"] %></h3>
                        </div>
                           

</div>
